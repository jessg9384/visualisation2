{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 600,
    "height": 450,
    "title": "Number of day trips to Australian regions and average expenditure in 2023",
    "projection": {"type": "mercator"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/jessg9384/week_9_homework/main/js/aus_states_with_graticules.topojson",
          "format": {"type": "topojson", "feature": "ne_50m_admin_1_states_provinces"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/jessg9384/week_9_homework/main/js/aus_states_with_graticules.topojson",
          "format": {"type": "topojson", "feature": "ne_50m_graticules_30"}
        },
        "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
      },

      {
        "data": {
          "url": "https://raw.githubusercontent.com/jessg9384/week_9_homework/main/data/visualisation2data.csv"
        },
        "mark": {"type": "circle","tooltip":true},
        "encoding": {
          "longitude": {"field": "long", "type": "quantitative"},
          "latitude": {"field": "lat", "type": "quantitative"},
          "size": {
            "field": "Number of trips (000)",
            "type": "quantitative",
            "title": "Number of trips ('000)",
            "scale": {"domain": [4000, 25000]}
          },
          "color": {
            "field": "Avg expenditure per trip (dollars) in year ending March 2023",
            "type": "quantitative",
            "title": "Average expenditure ($)",
            "scale": {"scheme": "blues"}
          },
          "tooltip": [
            {"field": "Region","type":"nominal","title":"Region"},
            {"field": "Rank","type":"ordinal","title":"Rank (by # of trips)"},
            {"field": "Number of trips (000)","type":"quantitative","title":"Number of trips ('000)","format":","},
            {"field": "Avg expenditure per trip (dollars) in year ending March 2023","type":"quantitative","title":"Average expenditure ($)"}
          ]
        }
        
      }
    ]
  }
